---
interface Props {
    highlighted: string;
    description: string;
}

const { highlighted, description } = Astro.props;
---


<div id="no-results" class="mt-2 hidden">
    <div class="p-4 mb-4 text-sm text-center text-blue-800 rounded-lg bg-blue-100 dark:bg-blue-100 dark:text-blue-400" role="alert">
      <span class="font-medium">{highlighted}</span> {description}
    </div>
</div>

  